/* Global Styles */
@font-face {
  font-family: 'Roboto';
  src: url("<%= asset_path 'Roboto-Regular.ttf' %>") format("truetype");
  font-weight: normal;
  font-style: normal;
}

html,
body {
  height: 100%;
  font-family: 'Roboto', sans-serif;
}

body {
  padding-top: 0px; /* Required padding for .navbar-fixed-top. Remove if using .navbar-static-top. Change if height of navigation changes. */
}

.img-portfolio {
  margin-bottom: 30px;
}

.img-hover:hover {
  opacity: 0.8;
}
a{
  outline: none!important;
  -webkit-transition: all .3s ease;
  -o-transition: all .3s ease;
  transition: all .3s ease;

}

/* Home Page Carousel */

header.carousel {
  height: 50%;
}

header.carousel .item,
header.carousel .item.active,
header.carousel .carousel-inner {
  height: 100%;
}

header.carousel .fill {
  width: 100%;
  height: 100%;
  background-position: center;
  background-size: cover;
}
a:focus, a:hover {
  outline: none;
  text-decoration: none;
}
.color-green {
  color: #49c630 !important;
}
.color-red {
  color: #f35656 !important;
}

.float-right{
  float:right;
}
/* 404 Page Styles */

.error-404 {
  font-size: 100px;
}
.padding80{
  padding:80px 0 80px 0 !important;
}

/* Pricing Page Styles */

.price {
  display: block;
  font-size: 50px;
  line-height: 50px;
}

.price sup {
  top: -20px;
  left: 2px;
  font-size: 20px;
}

.period {
  display: block;
  font-style: italic;
}

.green {
  background-color:yellowgreen !important;
}
.blue{
  background-color:teal !important;
}
/* Responsive Styles */

@media(max-width:991px) {
  .customer-img,
  .img-related {
    margin-bottom: 30px;
  }
}

@media(max-width:767px) {
  .main-menu {padding-right: 15px !important;
  padding-left: 15px !important;}
  #qui-champ, #button-search {padding:10px !important;}
  #searchform .row > div{
    padding:0;
    text-align: center;
  }
  .list-firms .listing-media {
    text-align: center;
    margin-bottom: 20px;
  }
  #call .right-section .feature .icon {
    text-align: center !important;
  }
  .affix .col-md-5 {
    width: 75% !important;
    padding-right: 0 !important;
  }
  .affix .col-md-2 {
    position: absolute;
    right: 48px;
  }
  .img-portfolio {
    margin-bottom: 15px;
  }
  .contact-info-blocks {
    padding: 30px 0 0 0 !important;
  }
  .contact-info-blocks > div a{
    float: none;
    display: block;
    text-align: center;
  }

  .contact-info-blocks > div{
    padding:0;
    margin:0;
  }
  ul.nav > li:first-child {
    display:none;
  }
  ul.nav > li{
    float:none !important;
  }
  ul.nav > li a{
    border-bottom: 1px solid rgba(0,0,0,.05);
  }
  ul.nav > li a:before{
    display: inline-block;
    font: normal normal normal 14px/1 FontAwesome;
    font-size: inherit;
    text-rendering: auto;
    content: "\f105";
    padding-right:10px;
    -webkit-font-smoothing: antialiased;
  }
  .navbar-nav {
    background:#fff;
    margin: 0px -15px 0 -15px;
  }
  .navbar-toggle{
    position: absolute;
    top: -50px;
    right: 0;
    font-size: 20px;
  }
  header.carousel .carousel {
    height: 70%;
  }
  .hero-section {
    height: auto !important;
    background: url(<%= asset_path 'bg.jpg' %>) no-repeat top center !important;

  }
  .featured-firm {
    margin-top: 170px !important;
  }
  .featured-firm .col-xs-12{
    text-align: center;
    margin-bottom: 20px;
  }
  .responsive:before {
    display:none !important;
  }
  .responsive:after {
    display:none !important;
  }

}

